{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "hartcode-api-nodeport"
  },
  "spec": {
    "selector": {
      "app": "hartcode-api"
    },
    "ports": [
      {
        "name": "hartcodeapihttp",
        "protocol": "TCP",
        "port": 80,
        "targetPort": 8811
      }
    ],
    "type": "NodePort"
  }
}
